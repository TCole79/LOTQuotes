<body class= "B-body">
<main class= "B-main">

<table class="table table-bordered">
<thead>
  <tr class "column-name">
    <th>Quote</th>
    <th>Author</th>
    <th>Edit</th>
    <th>Show</th>
  </tr>
</thead>
<tbody>

<% @quotes.each do |quote,index| %>

<tr>
  <td>
  <%= quote.quote_content %>
  </td>
  <td>
  <%= quote.quote_author %>
  </td>
  <td class= "edit-button-1">
  <%= link_to "Edit", edit_quote_path(quote) %>
  </td>
  <td class= "edit-button-2">
  <%= link_to "Show", quote %>
  </td>


<% end %>  

</main>
</body>







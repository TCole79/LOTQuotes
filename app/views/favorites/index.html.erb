<h1> Your Favorites </h1>

<div class="main-ring">
<img id="image" class="rotate2" src="<%= asset_path('ring2.png') %>" >
<div class="col-xs-12 col-sm-6">
  <div class="favorites">
    <% @favorite_quotes.all.each do |quote| %>
      <%= render 'quote',  quote: quote %>
    <% end %>
  </div>
</div>
</div>